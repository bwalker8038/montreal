{
  "manifest_version": 2,

  "name": "Montreal Build Monitor",
  "description": "This extension will kick off build a .NET application from the browser.",
  "version": "0.5",
  "permissions": [
    "https://10.25.4.1.194:4000/*",
    "notifications",
    "tabs",
    "storage"
  ],
  "background": {
    "page": "background.html"
  },
  "content_security_policy": "default-src 'none'; script-src 'self'; connect-src *",
  "browser_action": {
    "default_icon": "icon2.png",
    "default_popup": "index.html"
  } 
}
